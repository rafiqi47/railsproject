<%= render 'shared/error', obj: @category%>
  <%= form_with(model: @category, local: true) do |form| %>
  <div class="form-group"> 
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.submit :Submit, class: "btn btn-primary btn-block" %>
  </div>
<% end %>
